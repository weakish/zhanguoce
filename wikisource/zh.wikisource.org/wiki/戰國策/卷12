								<!-- subtitle -->
				<div id="contentSub"><span class="subpages">&lt; <a href="/wiki/%E6%88%B0%E5%9C%8B%E7%AD%96" title="戰國策">戰國策</a></span></div>
				<!-- /subtitle -->
																<!-- jumpto -->
				<div id="jump-to-nav" class="mw-jump">
