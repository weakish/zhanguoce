						<!-- firstHeading -->
			<h1 id="firstHeading" class="firstHeading"><span dir="auto">戰國策/卷12</span></h1>
			<!-- /firstHeading -->
			<!-- bodyContent -->
			<div id="bodyContent">
								<!-- tagline -->
				<div id="siteSub">維基文庫，自由的圖書館</div>
